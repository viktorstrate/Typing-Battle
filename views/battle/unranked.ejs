<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <title>Untitled Document</title>
    <link rel="stylesheet" href="/styles/battle.css">
    <link rel="stylesheet" href="/styles/style.css"/>

    <script src="//code.jquery.com/jquery-1.11.3.min.js"></script>
    <script src="//code.jquery.com/jquery-migrate-1.2.1.min.js"></script>

    <script src="/socket.io/socket.io.js"></script>
    <script src="/js/global.js"></script>
    <script src="/js/game/unranked.js"></script>
    <script src="/js/socket/unranked.js"></script>
</head>
<body>
<div class="container">



    <div id="score-border"></div>

    <div class="score-tower" id="your-score">
        <h2>0</h2>
    </div>

    <div class="score-tower" id="their-score">
        <h2>0</h2>
    </div>

    <div id="timer-container">
        <div id="time-left">Time Left</div>
        <div id="timer">0</div>
    </div>

    <di id="word-container">
        <div id="next-word"></div>
        <div id="main-word"><span id="first"></span><span id="last">Ready?</span></div>
    </di>

    <div id="popup">
        <div id="popup-window">
            <h2>Waiting for opponent to join.</h2>

            <p>Give this url to a friend, to start the game.</p>
        </div>
    </div>

</div>
</body>
</html>
